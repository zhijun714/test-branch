FROM java
MAINTAINER donghu
ARG RUNNING_ENVIRONMENT_$RUNNING_ENVIRONMENT
ENV RUNNING_ENVIRONMENT=RUNNING_ENVIRONMENT_
CMD echo "RUNNING_ENVIRONMENT is $RUNNING_ENVIRONMENT"
CMD echo "RUNNING_ENVIRONMENT is $RUNNING_ENVIRONMENT_"
VOLUME /tmp
ADD bi-app-1.0.0-SNAPSHOT-exec.jar app.jar
ENTRYPOINT ["java","-jar","/app.jar"]
